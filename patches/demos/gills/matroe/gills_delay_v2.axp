	Pbarpos=-1;
	barclock=-1;
	//kclock=0;
	//pkclock=0;
	kbarpos=0;
	bpmclock=0;
	bpmcount=0;
	active=1;
	
}
if (stop){
	active=0;
}







if (barpos!=Pbarpos){
	pulse=1;
	Pbarpos=barpos;
	if (barpos==0){
			barclock+=1;
			bartrig=1;
			
		
		}

		
}





int rawbpm=180000/kbeat;
float bpm_float = 180000.0f/kbeat;
int bpm_int = bpm_float;
int tenth=(bpm_float - bpm_int)*10;



if (tenth>=5){
	bpm=rawbpm+1;
}
else {
	bpm=rawbpm;
}
if (mode==0){
	setbpm=1;
}
if (setbpm){
	//bpm=inlet_bpm;
	stablebpm=bpm;
	solidbpm=bpm;
	prevbpm=bpm;
	change=0;
	setbpm=0;
	lastbpm=bpm;
	finalbpm=bpm;
}

if (pulse){
	if (bpm!=prevbpm){
		if (change>(0)){
			stablebpm=bpm;
		}
		if ( ((bpm>lastbpm)&&(bpm>prevbpm)) || ((bpm<lastbpm)&&(bpm<prevbpm)) ){
			change+=1;
		
		}
		else {
			change=0;
		}
		stable=0;
		//lastbpm=prevbpm;
		lastbpm=prevbpm;
		prevbpm=bpm;
		
	}
	else {
		if (change>(1)){
			stablebpm=bpm;
		}
		if (stable>(1)){
			stablebpm=bpm;
		
			
		}
		stable+=1;
		change=0;
	}

	if (stablebpm==pstable){
		if (pst<4){
			pst+=1;	
		}
		if (pst>2){
