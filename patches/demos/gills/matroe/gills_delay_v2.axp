	avg=180000/finalbpm;
	prevavg=avg;
}

if ((avg!=prevavg)&&(avg!=0X07FFFFFF)){
	prevavg=avg;
	
	
}

//midi CC
Xkbeat[1]=(sent1<<6)+sent2;

incount=(stepos/24);
k+=1;
if ((incount!=prevc)&&(stepos>=0)){
	
	Xbarpos[1]=incount-((incount>>2)<<2);
	if (stepos>0){
		Xkbeat[1]=k;
		sent1=k>>6;
		sent2=k-((k>>6)<<6);
	}
	prevc=incount;
	k=0;
}

if (inlet_reset&&!rtrig){
	rtrig=1;
	outcount=0;
	ppq1=0;
	
	//if (!active){
		Xstart[0]=1;
	//	}
