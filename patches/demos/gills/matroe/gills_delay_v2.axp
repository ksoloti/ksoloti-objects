		s=1;
		ntrig = 1;
//		save=0;
		i = 0;
		ii=0;
		previi=-1;
		src = 0 + start+range;
		dst = 0 + start;
			
		}
}

if (inlet_saveinitpreset && !strig){
	strig=1;
//	snapshot=inlet_snapshot+1;
	ntrig = 1;
//	save=1;
	i = 0;
	ii=0;
	previi=-1;
	src = 0 + start;
	dst = 0 + start + range;
	savinginit=1;


	
}
else if (!inlet_saveinitpreset){
