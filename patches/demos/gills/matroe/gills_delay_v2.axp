		mode=0;
	}
}
else if (!inlet_reset){
	rtrig=0;
}
//mode=0;

kbeat=Xkbeat[mode];
if (mode==1){
	ppq1=incount;
}
kbarpos=ppq1-((ppq1>>2)<<2);

