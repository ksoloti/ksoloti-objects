			if (checking==1){
				datapresent=1;
				checking=0;
				i=0;
			}
			else {
				datapresent=0;
				checking=0;
				active=0;
			}
		}
	}
	else if (!checking && datapresent){
		attr_table.array[__USAT((attr_offset+i),attr_table.LENGTHPOW)]=array[i]>>attr_table.GAIN;
		i+=1;
		if (i==attr_range){
			datapresent=0;
			active=0;
		}
	
		
