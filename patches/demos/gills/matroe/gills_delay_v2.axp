			timedn=1;
			timeup=0;
			if (inlet_octavefade){
			Pphase=(1<<(attr_size-inlet_fifth));
			}
		}
			
			if (!inlet_octavefade){
			Pphase=___SMMUL((0x07FFFFFF/param_pitchfade)<<3,length<<2);
			}
			
			
			
			if (Pphase<1){
				Pphase=1;
			}

		prevTA=timeA;
		}
		if (timeup){
			timeC+=Pphase;
			if (timeC>=prevTA){
				timeup=0;
				timeC=prevTA;
				}
			}
		else if (timedn){
			timeC-=Pphase;
			if (timeC<=prevTA){
				timedn=0;
				timeC=prevTA;
			}
		}
	
	}

timeD = ___SMMLA(timeD-timeC,(-1<<26)+(inlet_smooth>>1),timeD);

outlet_envA=outA;
if (!inlet_repitch){
	outlet_timeA=timeA;
	}
else {
	outlet_timeA=timeD;
}
outlet_envB=outB;
outlet_timeB=timeB;


//outlet_o=(inlet_i-((inlet_i/(0x07FFFFFF/param_entries))*(0x07FFFFFF/param_entries)))*param_entries;]]></code.krate>
